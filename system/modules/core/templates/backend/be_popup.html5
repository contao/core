    $objCombiner->add('assets/mootools/mootao/Mootao.js');
    $objCombiner->add('assets/contao/js/core.js');
    echo $objCombiner->getCombinedFile();
  ?>"></script>
  <script><?= $this->getDateString() ?></script>
  <?= $this->javascripts ?>
