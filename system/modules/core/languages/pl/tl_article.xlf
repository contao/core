        <target>Skasuj artykuł</target>
      </trans-unit>
      <trans-unit id="tl_article.delete.1">
        <source>Delete article ID %s</source>
        <target>Skasuj artykuł ID %s</target>
      </trans-unit>
      <trans-unit id="tl_article.toggle.0">
        <source>Publish/unpublish article</source>
        <target>Publikuj/nie publikuj artykuł</target>
      </trans-unit>
      <trans-unit id="tl_article.toggle.1">
        <source>Publish/unpublish article ID %s</source>
        <target>Publikuj/nie publikuj artykuł ID %s</target>
      </trans-unit>
      <trans-unit id="tl_article.pasteafter.0">
        <source>Paste after</source>
        <target>Wklej po</target>
      </trans-unit>
      <trans-unit id="tl_article.pasteafter.1">
        <source>Paste after article ID %s</source>
        <target>Wklej po artykule ID %s</target>
      </trans-unit>
      <trans-unit id="tl_article.pasteinto.0">
