        <target>Edytuj ustawienia artykułu</target>
      </trans-unit>
      <trans-unit id="tl_article.editheader.1">
        <source>Edit the settings of article ID %s</source>
        <target>Edytuj ustawienia artykułu ID %s</target>
      </trans-unit>
      <trans-unit id="tl_article.copy.0">
