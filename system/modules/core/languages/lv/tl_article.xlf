        <target>Pārskatījuma datums</target>
      </trans-unit>
      <trans-unit id="tl_article.tstamp.1">
        <source>Date and time of the latest revision</source>
        <target>Pēdējā pārskatījuma datums un laiks</target>
      </trans-unit>
