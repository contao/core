        <source>Username</source>
        <target>Používateľ</target>
      </trans-unit>
      <trans-unit id="tl_user.username.1">
        <source>Please enter a unique username.</source>
        <target>Prosím zadajte jedinečné užívateľské meno.</target>
      </trans-unit>
      <trans-unit id="tl_user.name.0">
        <source>Name</source>
        <target>Meno</target>
      </trans-unit>
