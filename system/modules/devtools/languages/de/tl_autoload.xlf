<?xml version="1.0" ?><xliff version="1.1">
  <file datatype="php" original="system/modules/devtools/languages/en/tl_autoload.php" source-language="en">
    <body>
      <trans-unit id="tl_autoload.headline">
        <source>Automatically create the autoload.php files</source>
        <target>Die autoload.php-Dateien automatisch erstellen</target>
      </trans-unit>
      <trans-unit id="tl_autoload.emptySelection">
        <source>Please select at least one module!</source>
        <target>Bitte wählen Sie mindestens ein Modul aus!</target>
      </trans-unit>
      <trans-unit id="tl_autoload.autoloadExists">
        <source>Module &quot;%s&quot; has an autoload.php file already. Do you want to override?</source>
        <target>Das Modul &quot;%s&quot; hat bereits eine autoload.php-Datei. Wollen Sie sie überschreiben?</target>
      </trans-unit>
      <trans-unit id="tl_autoload.available">
        <source>Available modules</source>
        <target>Verfügbare Module</target>
      </trans-unit>
      <trans-unit id="tl_autoload.xplAvailable">
        <source>Please select the modules you want to modify.</source>
        <target>Bitte wählen Sie die zu modifizierenden Module aus.</target>
      </trans-unit>
      <trans-unit id="tl_autoload.override">
        <source>Override existing files</source>
        <target>Bestehende Dateien überschreiben</target>
      </trans-unit>
      <trans-unit id="tl_autoload.xplOverride">
        <source>Override existing autoload.php files (use with care).</source>
        <target>Bestehende autoload.php-Dateien überschreiben (mit Bedacht verwenden).</target>
      </trans-unit>
      <trans-unit id="tl_autoload.autoload">
        <source>Create the autoload files</source>
        <target>Autoload-Dateien erstellen</target>
      </trans-unit>
      <trans-unit id="tl_autoload.autoloadConfirm">
        <source>The autoload.php file of module &quot;%s&quot; has been written.</source>
        <target>Die autoload.php-Datei des Moduls &quot;%s&quot; wurde erstellt.</target>
      </trans-unit>
      <trans-unit id="tl_autoload.ideCompat">
        <source>Update the IDE compatibility file</source>
        <target>IDE-Kompatibilitätsdatei aktualisieren</target>
      </trans-unit>
      <trans-unit id="tl_autoload.ideCompatConfirm">
        <source>The IDE compatibility file has been written.</source>
        <target>Die IDE-Kompatibilitätsdatei wurde erstellt.</target>
      </trans-unit>
    </body>
  </file>
