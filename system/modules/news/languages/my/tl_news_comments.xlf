<?xml version="1.0" ?><xliff version="1.1">
  <file datatype="php" original="system/modules/news/languages/en/tl_news_comments.php" source-language="en">
    <body>
      <trans-unit id="tl_news_comments.pid.0">
        <source>News item</source>
        <target>သတင်းပစ္စည်း</target>
      </trans-unit>
      <trans-unit id="tl_news_comments.pid.1">
        <source>The associated news item.</source>
        <target>ဆက်နွယ်ပြီးသတင်း</target>
      </trans-unit>
      <trans-unit id="tl_news_comments.date.0">
        <source>Date</source>
        <target>နေ့စွဲ</target>
      </trans-unit>
      <trans-unit id="tl_news_comments.date.1">
        <source>Please enter the comment date.</source>
        <target>မှတ်ချက်ချရက်စွဲထည့်သွင်း။</target>
      </trans-unit>
      <trans-unit id="tl_news_comments.name.0">
        <source>Author</source>
        <target>ရေးသားသူ</target>
      </trans-unit>
      <trans-unit id="tl_news_comments.name.1">
        <source>Please enter the author's name.</source>
        <target>ရေးသားသူနာမည်ထည့်သွင်း။</target>
      </trans-unit>
      <trans-unit id="tl_news_comments.email.0">
        <source>E-mail address</source>
        <target>အီးမေးလ်လိပ်စာ</target>
      </trans-unit>
      <trans-unit id="tl_news_comments.email.1">
        <source>The e-mail address will not be published.</source>
        <target>အီးမေးလ်လိပ်စာမဖြန့်ဝေပါ။</target>
      </trans-unit>
      <trans-unit id="tl_news_comments.website.0">
        <source>Website</source>
        <target>ဝဘ်ဆိုက်</target>
      </trans-unit>
      <trans-unit id="tl_news_comments.website.1">
        <source>Here you can enter a website address.</source>
        <target>ဝဘ်လိပ်စာကိုထည့်နိုင်။</target>
      </trans-unit>
      <trans-unit id="tl_news_comments.comment.0">
        <source>Comment</source>
        <target>မှတ်ချက်</target>
      </trans-unit>
      <trans-unit id="tl_news_comments.comment.1">
        <source>Please enter the comment.</source>
        <target>မှတ်ချက်ထည့်သွင်း။</target>
      </trans-unit>
      <trans-unit id="tl_news_comments.published.0">
        <source>Publish comment</source>
        <target>မှတ်ချက်ထုတ်ဝေ</target>
      </trans-unit>
      <trans-unit id="tl_news_comments.published.1">
        <source>Make the comment publicly visible on the website.</source>
        <target>ဝဘ်ဆိုက်ပေါ်တွင်မှတ်ချက်ကိုလူအများမြင်စေရန်လုပ်ဆောင်။</target>
      </trans-unit>
      <trans-unit id="tl_news_comments.author_legend">
        <source>Author</source>
        <target>ရေးသားသူ</target>
      </trans-unit>
      <trans-unit id="tl_news_comments.comment_legend">
        <source>Comment</source>
        <target>မှတ်ချက်</target>
      </trans-unit>
      <trans-unit id="tl_news_comments.publish_legend">
        <source>Publish settings</source>
        <target>ထုတ်ဝေချိန်ညှိချက်</target>
      </trans-unit>
      <trans-unit id="tl_news_comments.approved">
        <source>Approved</source>
        <target>လက်ခံပြီး</target>
      </trans-unit>
      <trans-unit id="tl_news_comments.pending">
        <source>Awaiting moderation</source>
        <target>စိစစ်မှုစောင့်ဆိုင်းနေ</target>
      </trans-unit>
      <trans-unit id="tl_news_comments.show.0">
        <source>Comment details</source>
        <target>မှတ်ချက်အချက်အလက်များ</target>
      </trans-unit>
      <trans-unit id="tl_news_comments.show.1">
        <source>Show the details of comment ID %s</source>
        <target>မှတ်ချက်အိုင်ဒီ %s အချက်အလက်များပြသ</target>
      </trans-unit>
      <trans-unit id="tl_news_comments.edit.0">
        <source>Edit comment</source>
        <target>မှတ်ချက်တည်းဖြတ်</target>
      </trans-unit>
      <trans-unit id="tl_news_comments.edit.1">
        <source>Edit comment ID %s</source>
        <target>မှတ်ချက်အိုင်ဒီ %s တည်းဖြတ်</target>
      </trans-unit>
      <trans-unit id="tl_news_comments.delete.0">
        <source>Delete comment</source>
        <target>မှတ်ချက်ပယ်ဖျက်</target>
      </trans-unit>
      <trans-unit id="tl_news_comments.delete.1">
        <source>Delete comment ID %s</source>
        <target>မှတ်ချက်အိုင်ဒီ %s ပယ်ဖျက်</target>
      </trans-unit>
      <trans-unit id="tl_news_comments.toggle.0">
        <source>Publish/unpublish comment</source>
        <target>မှတ်ချက်ထုတ်ဝေ/မထုတ်ဝေ</target>
      </trans-unit>
      <trans-unit id="tl_news_comments.toggle.1">
        <source>Publish/unpublish comment ID %s</source>
        <target>မှတ်ချက်အိုင်ဒီ %s ထုတ်ဝေ/မထုတ်ဝေ</target>
      </trans-unit>
      <trans-unit id="tl_news_comments.editheader.0">
        <source>Edit archive</source>
        <target>ဖိုင်ထုပ်တည်းဖြတ်</target>
      </trans-unit>
      <trans-unit id="tl_news_comments.editheader.1">
        <source>Edit the current archive</source>
        <target>လက်ရှိဖိုင်ထုပ်တည်းဖြတ်</target>
      </trans-unit>
    </body>
  </file>
