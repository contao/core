  <file datatype="php" original="system/modules/news/languages/en/tl_news_archive.php" source-language="en">
    <body>
      <trans-unit id="tl_news_archive.title.0">
        <source>Title</source>
        <target>Názov</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.title.1">
        <source>Please enter a news archive title.</source>
        <target>Prosím, vložte názov archívu noviniek.</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.jumpTo.0">
        <source>Redirect page</source>
        <target>Skočiť na stránku</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.jumpTo.1">
        <source>Please choose the news reader page to which visitors will be redirected when clicking a news item.</source>
        <target>Prosím vyberte stránku, na ktorú budú návštevníci presmerovaní po kliknutí na názov archívu noviniek.</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.allowComments.0">
        <source>Enable comments</source>
        <target>Povoliť komentáre</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.allowComments.1">
        <source>Allow visitors to comment news items.</source>
        <target>Povoliť návštevníkom komentovať novinky.</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.notify.0">
        <source>Notify</source>
        <target>Zaslať upozornenie</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.notify.1">
        <source>Please choose who to notify when comments are added.</source>
        <target>Zvoľte koho upozorniť, keď niekto vloží komentár.</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.sortOrder.0">
        <source>Sort order</source>
        <target>Zoradiť podľa</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.sortOrder.1">
        <source>By default, comments are sorted ascending, starting with the oldest one.</source>
        <target>Prednastavený spôsob zoradenia komentárov je vzostupne, začínajúc najstarším.</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.perPage.0">
        <source>Comments per page</source>
        <target>Položiek na stranu</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.perPage.1">
        <source>Number of comments per page. Set to 0 to disable pagination.</source>
        <target>Prosím zadajte číslo reprezentujúce počet komentárov na stránku (0 = vypnúť stránkovanie).</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.moderate.0">
        <source>Moderate comments</source>
        <target>Moderovať</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.moderate.1">
        <source>Approve comments before they are published on the website.</source>
        <target>Schváliť komentáre predtym, ako budú zobrazené na stránkach.</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.bbcode.0">
        <source>Allow BBCode</source>
        <target>Povoliť BBCode</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.bbcode.1">
        <source>Allow visitors to format their comments with BBCode.</source>
        <target>Povoliť návštevníkom používať BBCode k formátovaniu komentárov.</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.requireLogin.0">
        <source>Require login to comment</source>
        <target>Vyžadovať prihlásenie</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.requireLogin.1">
        <source>Allow only authenticated users to create comments.</source>
        <target>Neumožniť hosťom pridávať komentáre.</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.disableCaptcha.0">
        <source>Disable the security question</source>
        <target>Zakázať bezpečnostnú otázku</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.disableCaptcha.1">
        <source>Use this option only if you have limited comments to authenticated users.</source>
        <target>Vyberte túto možnosť k dekativovaniu bezpečnostnej otázky (nedoporučuje sa).</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.protected.0">
        <source>Protect archive</source>
        <target>Ochrániť archív</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.protected.1">
        <source>Show news items to certain member groups only.</source>
        <target>Zobraziť novinky iba určitej skupine.</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.groups.0">
        <source>Allowed member groups</source>
        <target>Povolené skupiny členov.</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.groups.1">
        <source>These groups will be able to see the news items in this archive.</source>
        <target>Tu môžete vybrať, ktorým skupinám bude umožnené prehliadať novinky.</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.tstamp.0">
        <source>Revision date</source>
        <target>Naposledy zmenené</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.tstamp.1">
        <source>Date and time of the latest revision</source>
        <target>Dátum a čas poslednej zmeny</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.title_legend">
        <source>Title and redirect page</source>
        <target>Názov a stránka presmerovania</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.comments_legend">
        <source>Comments</source>
        <target>Komentáre</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.protected_legend">
        <source>Access protection</source>
        <target>Chránený prístup</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.feed_legend">
        <source>RSS/Atom feed</source>
        <target>RSS/Atom kanál</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.notify_admin">
        <source>System administrator</source>
        <target>Administrátor systému</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.notify_author">
        <source>Author of the news item</source>
        <target>Autor novinky</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.notify_both">
        <source>Author and system administrator</source>
        <target>Autor a administrátor systému</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.source_teaser">
        <source>News teasers</source>
        <target>Ukážka novinky</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.source_text">
        <source>Full articles</source>
        <target>Plné znenie</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.new.0">
        <source>New archive</source>
        <target>Nový archív</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.new.1">
        <source>Create a new archive</source>
        <target>Vytvoriť nový archív</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.show.0">
        <source>Archive details</source>
        <target>Detaily archívu</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.show.1">
        <source>Show the details of archive ID %s</source>
        <target>Zobraziť detaily archívu s ID %s</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.edit.0">
        <source>Edit archive</source>
        <target>Upraviť archív</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.edit.1">
        <source>Edit archive ID %s</source>
        <target>Upraviť archív s ID %s</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.editheader.0">
        <source>Edit archive settings</source>
        <target>Upraviť nastavenia archívu</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.editheader.1">
        <source>Edit the settings of archive ID %s</source>
        <target>Upraviť nastavenia archívu s ID %s</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.copy.0">
        <source>Duplicate archive</source>
        <target>Kopírovať archív</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.copy.1">
        <source>Duplicate archive ID %s</source>
        <target>Kopírovať archív s ID %s</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.delete.0">
        <source>Delete archive</source>
        <target>Odstrániť archív</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.delete.1">
        <source>Delete archive ID %s</source>
        <target>Odstrániť archív s ID %s</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.comments.0">
        <source>Comments</source>
        <target>Komentáre</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.comments.1">
        <source>Show comments of archive ID %s</source>
        <target>Zobraziť komentáre k archívu s ID %s</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.feeds.0">
        <source>RSS feeds</source>
        <target>Kanály RSS</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.feeds.1">
        <source>Manage RSS feeds</source>
        <target>Spravovať kanály RSS</target>
      </trans-unit>
    </body>
