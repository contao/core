        <target>Autur da la novitad</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.notify_both">
        <source>Author and system administrator</source>
        <target>Autur ed administratur dal sistem</target>
      </trans-unit>
      <trans-unit id="tl_news_archive.source_teaser">
        <source>News teasers</source>
        <target>Teasers da novitads</target>
      </trans-unit>
